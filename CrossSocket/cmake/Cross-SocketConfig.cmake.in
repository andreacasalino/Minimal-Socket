get_filename_component(CROSSSOCK_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(CROSSSOCK_PROJECT_NAME Cross-Socket)
list(APPEND CMAKE_MODULE_PATH ${STREAMSOCK_CMAKE_DIR})
include(CMakeFindDependencyMacro)

